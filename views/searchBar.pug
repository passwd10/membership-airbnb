link(rel='stylesheet', href='/stylesheets/searchBar.css')
section(id='search_section')
  form(action=`/search?${place}` method='get' id='search_form')
    div(class='searchContent place')
      h5 위치
      input(type="text" name="place" placeholder="어디로 여행가세요?")
    div(class='searchContent checkin')
      h5 체크인
      span 날짜 추가
    div(class='searchContent checkout')
      h5 체크아웃
      span 날짜 추가
    div(class='searchContent personnel')
      h5 인원
      span 게스트 추가
    div(id='search_button')
      img(src='../images/find.svg' type="submit")
    //- button(type="submit" id="search_button") 검색